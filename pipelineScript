pipeline {
	agent any

	stages {
		stage('build'){

			steps{
				withMaven(maven: 'MVN353'){

				sh 'mvn clean compile -f ./mvn_project/pom.xml '
				} 
				
			}
		}
		// tests
		 
		// deploy
		stage('Deploy'){
			steps{
				withMaven(maven: 'MVN353'){
					sh 'mvn clean deploy -Dmaven.test.skip=true -f mvn_prj/pom.xml -s mvn_prj/conf/settings.xml'
				}			
			}
		}

	}


}